<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/com_visa_checkout_ten_px"
    >

    <ImageView
        android:id="@+id/com_visa_checkout_ivAlertIcon"
        android:layout_marginTop="@dimen/com_visa_checkout_pl_seventy_px"
        android:layout_width="@dimen/com_visa_checkout_eighty_px"
        android:layout_height="@dimen/com_visa_checkout_eighty_px"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/com_visa_checkout_layout_margin_left_right"
        android:scaleType="fitXY"
        android:src="@drawable/com_visa_checkout_icon_alert"
        android:visibility="gone" />

    <TextView
        android:id="@+id/com_visa_checkout_tvServerSideAlert_Message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/com_visa_checkout_ivAlertIcon"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/com_visa_checkout_twenty_px"
        android:layout_marginRight="@dimen/com_visa_checkout_layout_margin_left_right"
        android:layout_toRightOf="@id/com_visa_checkout_ivAlertIcon"
        android:text="@string/com_visa_checkout_tv_forgotPassword_error_not_found"
        android:textColor="@color/com_visa_checkout_tv_info_label_color"
        android:textSize="@dimen/com_visa_checkout_pl_twelve_px_sp"
        android:visibility="gone" />

